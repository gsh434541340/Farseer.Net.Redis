<?xml version="1.0"?>
<RedisConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ConnectTimeout>-1</ConnectTimeout>
  <SendTimeout>-1</SendTimeout>
  <ReceiveTimeout>-1</ReceiveTimeout>
  <IdleTimeOutSecs>240</IdleTimeOutSecs>
  <RetryTimeout>3000</RetryTimeout>
  <BackOffMultiplier>10</BackOffMultiplier>
  <BufferLength>1450</BufferLength>
  <BufferPoolMaxSize>500000</BufferPoolMaxSize>
  <VerifyMasterConnections>true</VerifyMasterConnections>
  <HostLookupTimeoutMs>200</HostLookupTimeoutMs>
  <AssumeServerVersion xsi:nil="true" />
  <DeactivatedClientsExpiry />
  <DisableVerboseLogging>false</DisableVerboseLogging>
</RedisConfig>